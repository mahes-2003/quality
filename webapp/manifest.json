{
  "_version": "1.60.0",
  "sap.app": {
    "id": "com.kaar.quality",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "15220b9d-cd8a-4e6a-ba91-0e268c828f0a"
    },
    "dataSources": {
      "ZQUALITY_2003_CDS_VAN": {
        "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='ZQUALITY_2003_CDS_VAN',Version='0001')/$value/",
        "type": "ODataAnnotation",
        "settings": {
          "localUri": "localService/mainService/ZQUALITY_2003_CDS_VAN.xml"
        }
      },
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZQUALITY_2003_CDS/",
        "type": "OData",
        "settings": {
          "annotations": [
            "ZQUALITY_2003_CDS_VAN"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "inspectionService": {
        "uri": "/sap/opu/odata/sap/ZINSPECTION_2003_CDS/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.14",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.kaar.quality.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "inspection": {
        "dataSource": "inspectionService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.kaar.quality.view",
        "async": true,
        "viewPath": "com.kaar.quality.view"
      },
      "routes": [
        {
          "name": "Login",
          "pattern": "",
          "target": [
            "TargetLogin"
          ]
        },
        {
          "name": "Dashboard",
          "pattern": "dashboard",
          "target": [
            "TargetDashboard"
          ]
        },
        {
          "name": "InspectionLotList",
          "pattern": "inspectionlots/:filter:",
          "target": [
            "TargetInspectionLotList"
          ]
        },
        {
          "name": "ResultRecording",
          "pattern": "resultrecording/{lotId}",
          "target": [
            "TargetResultRecording"
          ]
        },
        {
          "name": "UsageDecision",
          "pattern": "usagedecision/{lotId}",
          "target": [
            "TargetUsageDecision"
          ]
        }
      ],
      "targets": {
        "TargetLogin": {
          "id": "Login",
          "name": "Login"
        },
        "TargetDashboard": {
          "id": "Dashboard",
          "name": "Dashboard"
        },
        "TargetInspectionLotList": {
          "id": "InspectionLotList",
          "name": "InspectionLotList"
        },
        "TargetResultRecording": {
          "id": "ResultRecording",
          "name": "ResultRecording"
        },
        "TargetUsageDecision": {
          "id": "UsageDecision",
          "name": "UsageDecision"
        }
      }
    },
    "rootView": {
      "viewName": "com.kaar.quality.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}